var output2 = document.getElementById("output2");

function fizbiz(){
	var a = 1;
	while(a < 30){
	if (a % 15 = 0){
		output2 = "FizBiz" +<br>; 
	}
	else if(a % 5 = 0){
		output2 = "Biz" ;
	}
	else if (a % 3 = 0){
		output2 = "Fiz";
	}
	else{
		output2 = a ;
	}
	document.write = output2;
	a = a +1 ;
	}
}
